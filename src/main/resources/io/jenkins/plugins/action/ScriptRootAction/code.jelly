<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout" xmlns:st="jelly:stapler">
    
<h1>
    ${it.displayName}
</h1>

<p>
    Code page
</p>

<h1>
    Result
</h1>

<j:forEach items="${it.runScript(&quot;println 'hello world';def hello = 'Hallo Welt';&quot;)}" var="line">
    <p style="margin:0;padding:0;">
        ${line}
    </p>
</j:forEach>

</j:jelly>
